<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <OutputType>Library</OutputType>
        <RootNamespace>Simcube.AspNetCore.Modules.Microsoft</RootNamespace>
        <IsPackable>true</IsPackable>
        <Title>Simcube.AspNetCore.Modules</Title>
        <Authors>David Sekula</Authors>
        <Description>Minimal Api modules made simple</Description>
        <Copyright>Sim Cube Ltd 2022</Copyright>
        <PackageProjectUrl>https://github.com/SimCubeLtd/aspnetcore-modules</PackageProjectUrl>
        <PackageLicense>https://github.com/SimCubeLtd/aspnetcore-modules/blob/master/LICENSE.md</PackageLicense>
        <RepositoryUrl>https://github.com/SimCubeLtd/aspnetcore-modules</RepositoryUrl>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    </PropertyGroup>

</Project>